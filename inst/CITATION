citHeader("To cite trac in publications use:")

citEntry(
  entry    = "Article",
  title    = "Tree-aggregated predictive modeling of microbiome data",
  author   = c(person(given = "Jacob", family = "Bien"),
  person(given = "Xiaohan", family = "Yan"),
  person(given = "Léo", family = "Simpson"),
  person(given = "Christian L", family = "Müller")),
  journal  = "Scientific Reports",
  year     = 2021,
  volume   = 11,
  number   = 1,
  pages    = "1-13",
  textVersion = paste("Bien, J., Yan, X., Simpson, L., & Müller, C. L. (2021). Tree-aggregated predictive modeling of microbiome data. Scientific Reports, 11(1), 1-13."
  )
)
